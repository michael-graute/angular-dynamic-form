{
  "buttons": [
    {
      "key": "submit",
      "type": "submit",
      "label": "Submit",
      "icon": "bi-cloud-arrow-up",
      "class": "btn-success",
      "settings": {
        "disableIfFormInvalid": true
      },
      "callback": {
        "function": "formSubmit"
      }
    },
    {
      "key": "cancel",
      "type": "button",
      "label": "Cancel",
      "icon": "bi-x-circle",
      "class": "btn-danger",
      "callback": {
        "function": "formCancel"
      }
    },
    {
      "key": "reset",
      "type": "button",
      "label": "Reset",
      "icon": "bi-x-diamond",
      "class": "btn-warning",
      "callback": {
        "function": "formReset"
      }
    },
    {
      "key": "customCallback",
      "type": "button",
      "label": "Custom Callback",
      "icon": "bi-building-gear",
      "callback": {
        "function": "customCallback",
        "params": [
          "foo",
          "bar",
          1234,
          false
        ]
      }
    },
    {
      "key": "loadRemoteDate",
      "type": "button",
      "label": "Load Remote Data",
      "icon": "bi-cloud-arrow-down",
      "callback": {
        "function": "loadRemoteData",
        "params": {
          "url": "/get-form-data"
        }
      }
    }
  ],
  "elements": [
    {
      "key": "card-with-form-group",
      "label": "Card Container with Form Group",
      "type": "card",
      "children": [
        {
          "key": "repeater",
          "type": "repeater",
          "children": [
            {
              "type": "input",
              "key": "repeaterTextInput",
              "label": "Name 1",
              "settings": {
                "floatingLabel": true
              }
            },
            {
              "type": "input",
              "key": "repeaterEmailInput",
              "label": "Email",
              "controlType": "email",
              "settings": {
                "floatingLabel": true
              }
            }
          ]
        },
        {
          "key": "form-group",
          "type": "formGroup",
          "onChange": "formGroupChanged",
          "children": [
            {
              "key": "formGroupRow1",
              "type": "row",
              "children": [
                {
                  "key": "formGroupRow1Col1",
                  "type": "col",
                  "children": [
                    {
                      "type": "input",
                      "key": "formGroupName1",
                      "label": "Name 1",
                      "onChange": "doSomeFoo",
                      "settings": {
                        "floatingLabel": true
                      }
                    }
                  ]
                },
                {
                  "key": "formGroupRow1Col2",
                  "type": "col",
                  "children": [
                    {
                      "type": "input",
                      "key": "formGroupName2",
                      "label": "Name 2",
                      "settings": {
                        "floatingLabel": true
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "input",
              "key": "formGroupName3",
              "label": "Name 3",
              "settings": {
                "floatingLabel": true
              }
            }
          ]
        }
      ]
    },
    {
      "key": "email",
      "label": "E-Mail",
      "settings": {
        "floatingLabel": true
      },
      "type": "input",
      "controlType": "email",
      "validators": [
        {
          "name": "email"
        }
      ]
    },
    {
      "key": "fieldset1",
      "label": "Fieldset 1",
      "type": "fieldset",
      "children": [
        {
          "type": "input",
          "key": "name1",
          "label": "Name",
          "settings": {
            "floatingLabel": true
          },
          "validators": [
            {
              "name": "required"
            },
            {
              "name": "maxLength",
              "value": 12
            }
          ]
        }
      ]
    },
    {
      "key": "card",
      "label": "Card Container",
      "type": "card",
      "children": [
        {
          "type": "input",
          "key": "name2",
          "controlType": "text",
          "validators": [
            {
              "name": "required"
            },
            {
              "name": "maxLength",
              "value": 12
            }
          ]
        },
        {
          "type": "tabContainer",
          "key": "fieldset2TabContainer",
          "label": "Tab Container",
          "children": [
            {
              "type": "tabPane",
              "key": "tabPane1",
              "label": "Tab 1",
              "children": [
                {
                  "key": "tab1",
                  "type": "formGroup",
                  "children": [
                    {
                      "type": "row",
                      "key": "tabPane1row",
                      "children": [
                        {
                          "type": "col",
                          "key": "tabPane1rowCol1",
                          "class": "col-3",
                          "children": [
                            {
                              "type": "select",
                              "key": "name4",
                              "label": "Foo",
                              "onChange": "selectChanged",
                              "settings": {
                                "floatingLabel": true
                              },
                              "options": [
                                {
                                  "label": "One",
                                  "value": "one"
                                },
                                {
                                  "label": "Two",
                                  "value": "two"
                                }
                              ],
                              "validators": [
                                {
                                  "name": "required"
                                },
                                {
                                  "name": "maxLength",
                                  "value": 12
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "key": "tabPane1rowCol1",
                          "class": "col-9",
                          "children": [
                            {
                              "type": "input",
                              "key": "name3",
                              "controlType": "text",
                              "label": "Name 3",
                              "settings": {
                                "floatingLabel": true
                              },
                              "validators": [
                                {
                                  "name": "maxLength",
                                  "value": 12
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "tabPane",
              "key": "tabPane2",
              "label": "Tab 2",
              "children": [
                {
                  "key": "tab2",
                  "type": "formGroup",
                  "children": [
                    {
                    "type": "input",
                    "key": "tabPane2name5",
                    "controlType": "text",
                    "validators": [
                      {
                        "name": "required"
                      },
                      {
                        "name": "maxLength",
                        "value": 12
                      }
                    ]
                  }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "key": "key-value",
      "type": "key-value",
      "label": "Key-Value Input",
      "onChange": "keyValueChanged",
      "settings": {
        "colDefs": [
          {
            "header": "Some Foo",
            "type": "text",
            "varName": "foo"
          },
          {
            "header": "Some Bar",
            "type": "text",
            "varName": "bar"
          },
          {
            "header": "Some Baz",
            "type": "text",
            "varName": "baz"
          }
        ]
      },
      "value": [
        {
          "foo": null,
          "bar": null,
          "baz": null
        }
      ]
    },
    {
      "key": "data-relation",
      "type": "data-relation",
      "label": "Data Relation Input",
      "onChange": "dataRelationChanged",
      "settings": {
        "asyncUrl": "/get-related-form"
      }
    }
  ]
}
